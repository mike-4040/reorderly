{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "fix": "npm run lint && npm run format",
    "start": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --fix",
    "lint-ci": "eslint . --max-warnings 0",
    "format": "prettier --write .",
    "format-ci": "prettier --check .",
    "preview": "vite preview",
    "generate-routes": "tsr generate",
    "deploy": "npx firebase deploy --only hosting",
    "deploy-preview": "npx firebase hosting:channel:deploy preview",
    "secrets-dev": "doppler secrets -p web -c dev download --no-file --format env > .env",
    "secrets-stg": "doppler secrets -p web -c stg download --no-file --format env > .env",
    "secrets-prd": "doppler secrets -p web -c prd download --no-file --format env > .env"
  },
  "dependencies": {
    "@mantine/core": "^8.3.9",
    "@mantine/hooks": "^8.3.8",
    "@sentry/react": "^10.29.0",
    "@tanstack/react-router": "^1.139.14",
    "@tanstack/react-router-devtools": "^1.139.15",
    "firebase": "^12.6.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tanstack/eslint-plugin-router": "^1.139.0",
    "@tanstack/router-cli": "^1.139.14",
    "@tanstack/router-plugin": "^1.139.14",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "firebase-tools": "^14.27.0",
    "prettier": "^3.7.4",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6"
  }
}
